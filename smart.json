{
  "accordion": {
    "tailwind": {
      "config": {
        "theme": {
          "extend": {
            "keyframes": {
              "accordion-down": {
                "from": {
                  "height": "0"
                },
                "to": {
                  "height": "var(--radix-accordion-content-height)"
                }
              },
              "accordion-up": {
                "from": {
                  "height": "var(--radix-accordion-content-height)"
                },
                "to": {
                  "height": "0"
                }
              }
            },
            "animation": {
              "accordion-down": "accordion-down 0.2s ease-out",
              "accordion-up": "accordion-up 0.2s ease-out"
            }
          }
        }
      }
    }
  },
  "comp-01": {
    "meta": {
      "tags": ["input", "label"]
    }
  },
  "comp-02": {
    "meta": {
      "tags": ["input", "label", "required"]
    }
  },
  "comp-03": {
    "meta": {
      "tags": ["input", "label", "helper"]
    }
  }
}
